    {
      "name": "cfe025b5",
      "displayName": "trending_hashtags",
      "queryLines": [
        "with base as (select row_id, explode(hashtags_array) as hashtag from sentimental_analysis.gold.social_media_gold),\r\n",
        "final_data as (\r\n",
        "    select row_id, hashtag from base\r\n",
        "    group by row_id, hashtag\r\n",
        ")\r\n",
        "select hashtag, count(row_id) from final_data\r\n",
        "group by hashtag"
      ]
    }
